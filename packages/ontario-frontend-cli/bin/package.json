{
    "name": "kyle-ds-npm-test",
    "version": "0.0.1",
    "description": "Ontario.ca Frontend Project (TODO: Replace with a description of your project)",
    "main": "index.js",
    "scripts": {
        "build": "rm -rf dist && eleventy",
        "debug": "rm -rf dist && DEBUG=* eleventy",
        "serve": "eleventy --serve",
        "lint": "eslint . --ext .js",
        "format": "prettier --write .",
        "updateDs": "npm run mkDsDir && npm run cpDsFavicons && npm run cpDsFonts && npm run cpDsScripts",
        "mkDsDir": " rm -rf ./src/assets/vendor/ontario-design-system-2 && mkdir ./src/assets/vendor/ontario-design-system-2",
        "cpDsFavicons": "cp -a ./node_modules/@ongov/ontario-design-system-complete-styles/dist/favicons ./src/assets/vendor/ontario-design-system-2/favicons",
        "cpDsFonts": "cp -a ./node_modules/@ongov/ontario-design-system-complete-styles/dist/fonts ./src/assets/vendor/ontario-design-system-2/fonts",
        "cpDsScripts": "cp -a ./node_modules/@ongov/ontario-design-system-complete-styles/dist/scripts ./src/assets/vendor/ontario-design-system-2/scripts"
    },
    "author": "TODO changeme",
    "license": "Apache 2.0",
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@ongov/eslint-config-ontario-frontend": "^1.0.1",
        "@ongov/ontario-frontend": "latest",
        "@ongov/prettier-config-ontario-frontend": "latest",
        "eslint": "^8.57.0",
        "eslint-plugin-import": "^2.29.1",
        "prettier": "^3.2.5"
    },
    "dependencies": {
        "@ongov/ontario-design-system-complete-styles": "^4.2.0"
    }
}
